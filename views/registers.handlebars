    $(document).on("click","button.next",function(e) 
    {
        var curentnod = $(this).parent().parent().parent();
        var nextnode = $(this).parent().parent().parent().next();
        var dataids = $(this).parent().parent().parent().next().attr('data-id');

        curentnod.hide();
        nextnode.show();

        if(dataids == 'otp')
        {
            var morID = $('#morID').val();
            if(morID == "") {
                return false;
            }
            let timerOn = true;

        function timer(remaining) {
          //var m = Math.floor(remaining / 60);
          var s = remaining % 60;
          
          //m = m < 10 ? '0' + m : m;
          s = s < 0 ? '0' + s : s;
          document.getElementById('countdowntimer').innerHTML = s;
          remaining -= 1;
          
          if(remaining >= 0 && timerOn) {
            setTimeout(function() {
                timer(remaining);
            }, 1000);
            return;
          }

          if(!timerOn) {
            // Do validate stuff here
            return;
          }
          
          $('#timer').hide();
          $('#send_otp').addClass('active');
          
        }

        timer(60);        


           
        }

    });  
</script>

